{"ast":null,"code":"import request from \"@/utils/request\";\nimport moment from \"moment/moment\";\nexport default {\n  name: \"List\",\n  data() {\n    return {\n      tableData: [],\n      total: 0,\n      params: {\n        pageNum: 1,\n        pageSize: 10,\n        username: '',\n        isbn: '',\n        name: ''\n      }\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      request.get('retern/page', {\n        params: this.params\n      }).then(res => {\n        if (res.code === '200') {\n          this.tableData = res.data.list;\n          this.total = res.data.total;\n        }\n      });\n    },\n    del(row) {\n      const email = row.email;\n      const isbn = row.isbn;\n      request.delete('retern/delete/' + email + '&' + isbn).then(res => {\n        if (res.code === '200') {\n          this.$notify.success('Deleted');\n          this.load();\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    },\n    reset() {\n      this.params = {\n        pageNum: 1,\n        pageSize: 10,\n        email: '',\n        isbn: ''\n      };\n      this.value = '';\n      this.load();\n    },\n    changePageNum(pageNum) {\n      this.params.pageNum = pageNum;\n      this.load();\n    },\n    createDateFormat(row) {\n      return moment(row.cdate).format(\"YYYY-MM-DD\"); // a useful tool to format datetime\n    },\n    dueDateFormat(row) {\n      return moment(row.rdate).format(\"YYYY-MM-DD\");\n    },\n    returnDateFormat(row) {\n      return moment(row.adate).format(\"YYYY-MM-DD\");\n    }\n  }\n};","map":{"version":3,"names":["request","moment","name","data","tableData","total","params","pageNum","pageSize","username","isbn","created","load","methods","get","then","res","code","list","del","row","email","delete","$notify","success","error","msg","reset","value","changePageNum","createDateFormat","cdate","format","dueDateFormat","rdate","returnDateFormat","adate"],"sources":["src/views/retern/List.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!-- search area -->\r\n    <div style=\"margin-bottom: 2px; margin-top: 2px\">\r\n      <el-input v-model=\"params.username\" placeholder=\"Enter username\" style=\"width: 200px; margin-left: 2px\"></el-input>\r\n      <el-input v-model=\"params.name\" placeholder=\"Enter book's name\" style=\"width: 200px; margin-left: 2px\"></el-input>\r\n      <el-input v-model=\"params.isbn\" placeholder=\"Enter book's isbn\" style=\"width: 200px; margin-left: 2px\"></el-input>\r\n      <el-button type=\"primary\" style=\"margin-left: 2px; height: 40px\" icon=\"el-icon-search\" @click=\"load\">Search</el-button>\r\n      <el-button type=\"warning\" style=\"margin-left: 2px; height: 40px\" icon=\"el-icon-refresh-right\" @click=\"reset\">Reset</el-button>\r\n    </div>\r\n    <!-- table area -->\r\n    <div>\r\n      <el-table :data=\"tableData\" style=\"width: 100%\" stripe>\r\n        <el-table-column prop=\"uid\" label=\"User ID\" show-overflow-tooltip width=\"150\"></el-table-column>\r\n        <el-table-column prop=\"username\" label=\"Username\" show-overflow-tooltip width=\"120\"></el-table-column>\r\n        <el-table-column prop=\"phone\" label=\"Phone\" width=\"100\"></el-table-column>\r\n        <el-table-column prop=\"name\" label=\"Book Name\" show-overflow-tooltip width=\"450\"></el-table-column>\r\n        <el-table-column prop=\"isbn\" label=\"ISBN\" width=\"100\"></el-table-column>\r\n        <el-table-column prop=\"bstatus\" label=\"Status\" width=\"100\"></el-table-column>\r\n        <el-table-column prop=\"cdate\" label=\"Borrow Date\" width=\"120\" :formatter=\"createDateFormat\"></el-table-column>\r\n        <el-table-column prop=\"duration\" label=\"Days\" width=\"100\"></el-table-column>\r\n        <el-table-column prop=\"rdate\" label=\"Due Date\" width=\"100\" :formatter=\"dueDateFormat\"></el-table-column>\r\n        <el-table-column prop=\"adate\" label=\"Return Date\" width=\"180\" :formatter=\"returnDateFormat\"></el-table-column>\r\n        <el-table-column fixed=\"right\" label=\"Operations\">\r\n          <template v-slot=\"scope\">\r\n            <el-popconfirm\r\n                confirm-button-text='Yes'\r\n                cancel-button-text='No'\r\n                title=\"Are you sure you want to delete this row of dataï¼Ÿ\"\r\n                @confirm=\"del(scope.row)\"\r\n            >\r\n              <el-button style=\"margin-left: 2px;\" slot=\"reference\" type=\"danger\">Delete</el-button>\r\n            </el-popconfirm>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <!-- page -->\r\n      <el-pagination\r\n          style=\"margin-top: 5px;\"\r\n          background\r\n          :current-page=\"params.pageNum\"\r\n          :page-size=\"params.pageSize\"\r\n          @current-change=\"changePageNum\"\r\n          layout=\"prev, pager, next\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\nimport moment from \"moment/moment\";\r\n\r\nexport default {\r\n  name: \"List\",\r\n\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      total: 0,\r\n      params: {\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n        username: '',\r\n        isbn: '',\r\n        name: ''\r\n      },\r\n    }\r\n  },\r\n\r\n  created() {\r\n    this.load()\r\n  },\r\n\r\n  methods: {\r\n    load() {\r\n      request.get('retern/page', {\r\n        params: this.params\r\n      }).then(res => {\r\n        if(res.code === '200') {\r\n          this.tableData = res.data.list\r\n          this.total = res.data.total\r\n        }\r\n      })\r\n    },\r\n\r\n    del(row) {\r\n      const email = row.email\r\n      const isbn = row.isbn\r\n\r\n      request.delete('retern/delete/' + email + '&' + isbn).then(res => {\r\n        if(res.code === '200') {\r\n          this.$notify.success('Deleted')\r\n          this.load()\r\n        } else {\r\n          this.$notify.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n\r\n    reset() {\r\n      this.params = {\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n        email: '',\r\n        isbn: '',\r\n      }\r\n      this.value = ''\r\n      this.load()\r\n    },\r\n\r\n    changePageNum(pageNum) {\r\n      this.params.pageNum = pageNum\r\n      this.load()\r\n    },\r\n\r\n    createDateFormat(row) {\r\n      return moment(row.cdate).format(\"YYYY-MM-DD\") // a useful tool to format datetime\r\n    },\r\n    dueDateFormat(row) {\r\n      return moment(row.rdate).format(\"YYYY-MM-DD\")\r\n    },\r\n    returnDateFormat(row) {\r\n      return moment(row.adate).format(\"YYYY-MM-DD\")\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":"AAmDA,OAAAA,OAAA;AACA,OAAAC,MAAA;AAEA;EACAC,IAAA;EAEAC,KAAA;IACA;MACAC,SAAA;MACAC,KAAA;MACAC,MAAA;QACAC,OAAA;QACAC,QAAA;QACAC,QAAA;QACAC,IAAA;QACAR,IAAA;MACA;IACA;EACA;EAEAS,QAAA;IACA,KAAAC,IAAA;EACA;EAEAC,OAAA;IACAD,KAAA;MACAZ,OAAA,CAAAc,GAAA;QACAR,MAAA,OAAAA;MACA,GAAAS,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAb,SAAA,GAAAY,GAAA,CAAAb,IAAA,CAAAe,IAAA;UACA,KAAAb,KAAA,GAAAW,GAAA,CAAAb,IAAA,CAAAE,KAAA;QACA;MACA;IACA;IAEAc,IAAAC,GAAA;MACA,MAAAC,KAAA,GAAAD,GAAA,CAAAC,KAAA;MACA,MAAAX,IAAA,GAAAU,GAAA,CAAAV,IAAA;MAEAV,OAAA,CAAAsB,MAAA,oBAAAD,KAAA,SAAAX,IAAA,EAAAK,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAM,OAAA,CAAAC,OAAA;UACA,KAAAZ,IAAA;QACA;UACA,KAAAW,OAAA,CAAAE,KAAA,CAAAT,GAAA,CAAAU,GAAA;QACA;MACA;IACA;IAEAC,MAAA;MACA,KAAArB,MAAA;QACAC,OAAA;QACAC,QAAA;QACAa,KAAA;QACAX,IAAA;MACA;MACA,KAAAkB,KAAA;MACA,KAAAhB,IAAA;IACA;IAEAiB,cAAAtB,OAAA;MACA,KAAAD,MAAA,CAAAC,OAAA,GAAAA,OAAA;MACA,KAAAK,IAAA;IACA;IAEAkB,iBAAAV,GAAA;MACA,OAAAnB,MAAA,CAAAmB,GAAA,CAAAW,KAAA,EAAAC,MAAA;IACA;IACAC,cAAAb,GAAA;MACA,OAAAnB,MAAA,CAAAmB,GAAA,CAAAc,KAAA,EAAAF,MAAA;IACA;IACAG,iBAAAf,GAAA;MACA,OAAAnB,MAAA,CAAAmB,GAAA,CAAAgB,KAAA,EAAAJ,MAAA;IACA;EACA;AACA","ignoreList":[]},"metadata"