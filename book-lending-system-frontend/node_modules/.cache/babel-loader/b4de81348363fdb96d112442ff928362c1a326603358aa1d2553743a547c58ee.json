{"ast":null,"code":"import request from \"@/utils/request\";\nexport default {\n  name: \"Register\",\n  data() {\n    const checkPhone = (rule, value, callback) => {\n      if (!value) {\n        callback(new Error(\"請輸入手機號碼\"));\n      }\n      if (!/^[1,2,3,4,5,6,7,8,9][0-9]{9}$/.test(value)) {\n        callback(new Error(\"非法的手機號碼\"));\n      }\n      callback();\n    };\n    return {\n      form: {},\n      rules: {\n        phone: [{\n          required: true,\n          validator: checkPhone,\n          trigger: \"blur\"\n        }],\n        password: [{\n          required: true,\n          message: \"請輸入密碼\",\n          trigger: \"blur\"\n        }],\n        username: [{\n          required: true,\n          message: \"請輸入使用者名稱\",\n          trigger: \"blur\"\n        }]\n      }\n    };\n  },\n  methods: {\n    save() {\n      this.$refs[\"ruleForm\"].validate(valid => {\n        if (valid) {\n          request.post(\"user/save\", this.form).then(res => {\n            if (res.code === \"200\") {\n              this.$notify.success(\"提交成功\");\n              this.$refs[\"ruleForm\"].resetFields();\n            } else {\n              this.$notify.error(res.msg);\n            }\n          });\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["request","name","data","checkPhone","rule","value","callback","Error","test","form","rules","phone","required","validator","trigger","password","message","username","methods","save","$refs","validate","valid","post","then","res","code","$notify","success","resetFields","error","msg"],"sources":["src/views/register/Register.vue"],"sourcesContent":["<template>\r\n    <div class=\"register-container\">\r\n      <div class=\"register-form\">\r\n        <h1 class=\"register-title\">註冊</h1>\r\n        <el-form :model=\"form\" :rules=\"rules\" ref=\"ruleForm\">\r\n          <el-form-item label=\"手機號碼：\" prop=\"phone\">\r\n            <el-input v-model=\"form.phone\" placeholder=\"請輸入手機號碼\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"密碼：\" prop=\"password\">\r\n            <el-input v-model=\"form.password\" type=\"password\" placeholder=\"請輸入密碼\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"使用者名稱：\" prop=\"username\">\r\n            <el-input v-model=\"form.username\" placeholder=\"請輸入使用者名稱\"></el-input>\r\n          </el-form-item>\r\n        </el-form>\r\n        <div class=\"register-button\">\r\n          <el-button type=\"primary\" @click=\"save\">下一步</el-button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import request from \"@/utils/request\";\r\n  \r\n  export default {\r\n    name: \"Register\",\r\n    data() {\r\n      const checkPhone = (rule, value, callback) => {\r\n        if (!value) {\r\n          callback(new Error(\"請輸入手機號碼\"));\r\n        }\r\n        if (!/^[1,2,3,4,5,6,7,8,9][0-9]{9}$/.test(value)) {\r\n          callback(new Error(\"非法的手機號碼\"));\r\n        }\r\n        callback();\r\n      };\r\n  \r\n      return {\r\n        form: {},\r\n        rules: {\r\n          phone: [{ required: true, validator: checkPhone, trigger: \"blur\" }],\r\n          password: [{ required: true, message: \"請輸入密碼\", trigger: \"blur\" }],\r\n          username: [\r\n            { required: true, message: \"請輸入使用者名稱\", trigger: \"blur\" },\r\n          ],\r\n        },\r\n      };\r\n    },\r\n  \r\n    methods: {\r\n      save() {\r\n        this.$refs[\"ruleForm\"].validate((valid) => {\r\n          if (valid) {\r\n            request.post(\"user/save\", this.form).then((res) => {\r\n              if (res.code === \"200\") {\r\n                this.$notify.success(\"提交成功\");\r\n                this.$refs[\"ruleForm\"].resetFields();\r\n              } else {\r\n                this.$notify.error(res.msg);\r\n              }\r\n            });\r\n          }\r\n        });\r\n      },\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .register-container {\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    height: 100vh;\r\n  }\r\n  \r\n  .register-form {\r\n    width: 400px;\r\n    padding: 20px;\r\n    border-radius: 5px;\r\n    background-color: #f0f2f5;\r\n    box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\r\n  }\r\n  \r\n  .register-title {\r\n    text-align: center;\r\n    font-size: 24px;\r\n    margin-bottom: 20px;\r\n  }\r\n  \r\n  .register-button {\r\n    text-align: center;\r\n    margin-top: 20px;\r\n  }\r\n  </style>\r\n  "],"mappings":"AAuBA,OAAAA,OAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA,MAAAC,UAAA,GAAAA,CAAAC,IAAA,EAAAC,KAAA,EAAAC,QAAA;MACA,KAAAD,KAAA;QACAC,QAAA,KAAAC,KAAA;MACA;MACA,qCAAAC,IAAA,CAAAH,KAAA;QACAC,QAAA,KAAAC,KAAA;MACA;MACAD,QAAA;IACA;IAEA;MACAG,IAAA;MACAC,KAAA;QACAC,KAAA;UAAAC,QAAA;UAAAC,SAAA,EAAAV,UAAA;UAAAW,OAAA;QAAA;QACAC,QAAA;UAAAH,QAAA;UAAAI,OAAA;UAAAF,OAAA;QAAA;QACAG,QAAA,GACA;UAAAL,QAAA;UAAAI,OAAA;UAAAF,OAAA;QAAA;MAEA;IACA;EACA;EAEAI,OAAA;IACAC,KAAA;MACA,KAAAC,KAAA,aAAAC,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACAtB,OAAA,CAAAuB,IAAA,mBAAAd,IAAA,EAAAe,IAAA,CAAAC,GAAA;YACA,IAAAA,GAAA,CAAAC,IAAA;cACA,KAAAC,OAAA,CAAAC,OAAA;cACA,KAAAR,KAAA,aAAAS,WAAA;YACA;cACA,KAAAF,OAAA,CAAAG,KAAA,CAAAL,GAAA,CAAAM,GAAA;YACA;UACA;QACA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}