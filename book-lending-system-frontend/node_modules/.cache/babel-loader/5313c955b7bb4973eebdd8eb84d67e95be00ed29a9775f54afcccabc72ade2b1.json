{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport request from \"@/utils/request\";\nimport Cookies from \"js-cookie\";\nexport default {\n  name: \"List\",\n  data() {\n    return {\n      admin: Cookies.get('admin') ? JSON.parse(Cookies.get('admin')) : {},\n      tableData: [],\n      total: 0,\n      form: {},\n      params: {\n        pageNum: 1,\n        pageSize: 10,\n        email: '',\n        username: ''\n      }\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      request.get('admin/page', {\n        params: this.params\n      }).then(res => {\n        if (res.code === '200') {\n          this.tableData = res.data.list;\n          this.total = res.data.total;\n        }\n      });\n    },\n    reset() {\n      this.params = {\n        pageNum: 1,\n        pageSize: 10,\n        email: '',\n        username: ''\n      };\n      this.load();\n    },\n    del(email) {\n      request.delete('admin/delete/' + email).then(res => {\n        if (res.code === '200') {\n          this.$notify.success('Deleted');\n          this.load();\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    },\n    resetPass(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      // console.log(this.form)\n      request.put('admin/resetPass', this.form).then(res => {\n        if (res.code === '200') {\n          this.$notify.success('Password reset');\n          if (this.form.email === this.admin.email) {\n            Cookies.remove('admin');\n            this.$router.push('/login');\n          }\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    },\n    changeStatus(row) {\n      if (this.admin.email === row.email && !row.status) {\n        this.$notify.warning(\"Illegal Operation\");\n        row.status = true;\n      } else {\n        // console.log(row)\n        request.put('admin/update', row).then(res => {\n          if (res.code === '200') {\n   