     </el-table>\r\n      <!-- page -->\r\n      <el-pagination\r\n          style=\"margin-top: 5px;\"\r\n          background\r\n          :current-page=\"params.pageNum\"\r\n          :page-size=\"params.pageSize\"\r\n          @current-change=\"changePageNum\"\r\n          layout=\"prev, pager, next\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\nimport moment from \"moment\";\r\n\r\nexport default {\r\n  name: \"List\",\r\n\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      total: 0,\r\n      params: {\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n        username: '',\r\n        isbn: '',\r\n        name: ''\r\n      },\r\n    }\r\n  },\r\n\r\n  created() {\r\n    this.load()\r\n    // console.log(moment(\"2023-01-05 23:12:22\").format(\"YYYY-MM-DD\"))\r\n  },\r\n\r\n  methods: {\r\n    load() {\r\n      request.get('borrow/page', {\r\n        params: this.params\r\n      }).then(res => {\r\n        if(res.code === '200') {\r\n          this.tableData = res.data.list\r\n          this.total = res.data.total\r\n        }\r\n      })\r\n    },\r\n\r\n    del(row) {\r\n      const email = row.email\r\n      const isbn = row.isbn\r\n      const id = row.id\r\n\r\n      request.delete('borrow/delete/' + email + '&' + isbn + '&' + id).then(res => {\r\n        if(res.code === '200') {\r\n          this.$notify.success('Deleted')\r\n          this.load()\r\n        } else {\r\n          this.$notify.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n\r\n    reset() {\r\n      this.params = {\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n        email: '',\r\n        isbn: '',\r\n      }\r\n      this.value = ''\r\n      this.load()\r\n    },\r\n\r\n    changePageNum(pageNum) {\r\n      this.params.pageNum = pageNum\r\n      this.load()\r\n    },\r\n\r\n    bookReturn(row) {\r\n      // console.log(row)\r\n      request.post('/retern/save', row).then(res => {\r\n        if(res.code === '200') {\r\n          this.$notify.success('Book returned')\r\n          location.reload()\r\n        } else {\r\n          this.$notify.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n\r\n    createDateFormat(row) {\r\n      return moment(row.cdate).format(\"YYYY-MM-DD\") // a useful tool to format datetime\r\n    },\r\n    dueDateFormat(row) {\r\n      return moment(row.rdate).format(\"YYYY-MM-DD\")\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":"AAyEA,OAAAA,OAAA;AACA,OAAAC,MAAA;AAEA;EACAC,IAAA;EAEAC,KAAA;IACA;MACAC,SAAA;MACAC,KAAA;MACAC,MAAA;QACAC,OAAA;QACAC,QAAA;QACAC,QAAA;QACAC,IAAA;QACAR,IAAA;MACA;IACA;EACA;EAEAS,QAAA;IACA,KAAAC,IAAA;IACA;EACA;EAEAC,OAAA;IACAD,KAAA;MACAZ,OAAA,CAAAc,GAAA;QACAR,MAAA,OAAAA;MACA,GAAAS,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAb,SAAA,GAAAY,GAAA,CAAAb,IAAA,CAAAe,IAAA;UACA,KAAAb,KAAA,GAAAW,GAAA,CAAAb,IAAA,CAAAE,KAAA;QACA;MACA;IACA;IAEAc,IAAAC,GAAA;MACA,MAAAC,KAAA,GAAAD,GAAA,CAAAC,KAAA;MACA,MAAAX,IAAA,GAAAU,GAAA,CAAAV,IAAA;MACA,MAAAY,EAAA,GAAAF,GAAA,CAAAE,EAAA;MAEAtB,OAAA,CAAAuB,MAAA,oBAAAF,KAAA,SAAAX,IAAA,SAAAY,EAAA,EAAAP,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAO,OAAA,CAAAC,OAAA;UACA,KAAAb,IAAA;QACA;UACA,KAAAY,OAAA,CAAAE,KAAA,CAAAV,GAAA,CAAAW,GAAA;QACA;MACA;IACA;IAEAC,MAAA;MACA,KAAAtB,MAAA;QACAC,OAAA;QACAC,QAAA;QACAa,KAAA;QACAX,IAAA;MACA;MACA,KAAAmB,KAAA;MACA,KAAAjB,IAAA;IACA;IAEAkB,cAAAvB,OAAA;MACA,KAAAD,MAAA,CAAAC,OAAA,GAAAA,OAAA;MACA,KAAAK,IAAA;IACA;IAEAmB,WAAAX,GAAA;MACA;MACApB,OAAA,CAAAgC,IAAA,iBAAAZ,GAAA,EAAAL,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAO,OAAA,CAAAC,OAAA;UACAQ,QAAA,CAAAC,MAAA;QACA;UACA,KAAAV,OAAA,CAAAE,KAAA,CAAAV,GAAA,CAAAW,GAAA;QACA;MACA;IACA;IAEAQ,iBAAAf,GAAA;MACA,OAAAnB,MAAA,CAAAmB,GAAA,CAAAgB,KAAA,EAAAC,MAAA;IACA;IACAC,cAAAlB,GAAA;MACA,OAAAnB,MAAA,CAAAmB,GAAA,CAAAmB,KAAA,EAAAF,MAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}