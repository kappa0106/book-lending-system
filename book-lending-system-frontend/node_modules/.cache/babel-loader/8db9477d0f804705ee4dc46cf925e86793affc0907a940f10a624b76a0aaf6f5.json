   this.load();\r\n    },\r\n\r\n    changePageNum(pageNum) {\r\n      this.params.pageNum = pageNum;\r\n      this.load();\r\n    },\r\n    \r\n    returnBook(row) {\r\n      // Simulating return book action\r\n      // You should replace this with your actual API request\r\n      row.returned = true;\r\n      // Call API to update return date and book status\r\n      request.post(`/books/${row.isbn}/return`)\r\n        .then(() => {\r\n          this.$message.success('Book returned successfully.');\r\n        })\r\n        .catch(error => {\r\n          this.$message.error(`Failed to return book: ${error.message}`);\r\n        });\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"mappings":"AA6BA,OAAAA,OAAA;AAEA;EACAC,IAAA;EAEAC,KAAA;IACA;MACAC,SAAA;MACAC,KAAA;MACAC,MAAA;QACAC,OAAA;QACAC,QAAA;MACA;IACA;EACA;EAEAC,QAAA;IACA,KAAAC,IAAA;EACA;EAEAC,OAAA;IACAD,KAAA;MACA;MACA;MACA,KAAAN,SAAA,IACA;QAAAF,IAAA;QAAAU,IAAA;QAAAC,WAAA;QAAAC,QAAA;MAAA,GACA;QAAAZ,IAAA;QAAAU,IAAA;QAAAC,WAAA;QAAAC,QAAA;MAAA,GACA;QAAAZ,IAAA;QAAAU,IAAA;QAAAC,WAAA;QAAAC,QAAA;MAAA,EACA;IACA;IAEAC,MAAA;MACA,KAAAT,MAAA;QACAC,OAAA;QACAC,QAAA;MACA;MACA,KAAAE,IAAA;IACA;IAEAM,cAAAT,OAAA;MACA,KAAAD,MAAA,CAAAC,OAAA,GAAAA,OAAA;MACA,KAAAG,IAAA;IACA;IAEAO,WAAAC,GAAA;MACA;MACA;MACAA,GAAA,CAAAJ,QAAA;MACA;MACAb,OAAA,CAAAkB,IAAA,WAAAD,GAAA,CAAAN,IAAA,WACAQ,IAAA;QACA,KAAAC,QAAA,CAAAC,OAAA;MACA,GACAC,KAAA,CAAAC,KAAA;QACA,KAAAH,QAAA,CAAAG,KAAA,2BAAAA,KAAA,CAAAC,OAAA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}